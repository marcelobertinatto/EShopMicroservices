    "https": {
      "commandName": "Project",
      "launchUrl": "basket",
      "applicationUrl": "https://localhost:5051;http://localhost:5001",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true
